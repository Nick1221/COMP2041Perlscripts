#!/bin/bash
#Testing case and comments at the end of line.
#Created by Nicholas Leung, we all love fruits

FRUIT="kiwi"

case "$FRUIT" in
   "apple") echo "Apple pie is quite tasty."   #I agree
   ;;
   "banana") echo "I like banana nut bread."   #Hmmm maybe not
   ;;
   "kiwi") echo "New Zealand is famous for kiwi." #dat tru
   ;;
esac
